@model IEnumerable<FFF.Models.OrderSystem.Order>

<div class="row panel panel-primary">
	@if ( Model.Count() == 0 )
	{
		<div class="well well-lg">
			<p class="text-center"><strong>You have no Orders.</strong> If you place an Order, you can access and edit it here.</p>
		</div>
	}
	else
	{
		foreach ( var order in Model )
		{
			<div class="well well-sm col-sm-3" id="Display-@order.RID">
				@{ Html.RenderPartial( "_Order", order ); }
			</div>
		}
	}
</div>







