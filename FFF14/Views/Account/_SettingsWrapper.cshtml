<div data-bind="with: Page.Account.Settings">
	@{
		//Html.RenderPartial( "_Setting" );
		//Html.RenderPartial( "_SettingCreate" );
		//Html.RenderPartial( "_SettingEdit" );
		//Html.RenderPartial( "_SettingDelete" );
	}
	<div id="Settings" class="subcontainer">
		<div class="row subheader">
			<div class="subtitle">
				<h2>
					<span class="glyphicon glyphicon-wrench">
					</span>
					Settings
				</h2>
			</div>
		</div>
		<div id="AddressesContent" class="subcontent">
			<div class="row panel panel-primary">
				<div data-bind="if: Page.Account.Settings.empty">
					<div data-bind="if:Page.Account.Settings.loading">
						<div class="well well-lg">
							<p class="text-center">
								<img src="@Url.Content("~/Content/Images/ajax-loader.gif")" width="50px" height="50px" />
								<strong>We are updating your Settings.</strong>
								This should only take a moment.
							</p>
						</div>
					</div>
					<div data-bind="ifnot:Page.Account.Settings.loading">
						<div class="well well-lg">
							<p class="text-center">
								<strong>You have no Settings.</strong> If you add an Settings, you can view, edit, and delete it here.
							</p>
						</div>
					</div>
				</div>
				<div data-bind="ifnot: Page.Account.Settings.empty">
					<div data-bind="template: { name: Page.Account.Settings.displayMode, foreach: Page.Account.Settings.list }">
					</div>
				</div>
			</div>
		</div>
	</div>
</div>